      "estimated_hours": 8.0,
      "default_assignee": null,
      "assignee_key": "implementation"
    },
    {
      "subject_template": "[Testing] {translated_summary}",
      "estimated_hours": 4.0,
      "default_assignee": null,
      "assignee_key": "testing"
    },
    {
      "subject_template": "[Code Review] {translated_summary}",
      "estimated_hours": 2.0,
      "default_assignee": null,
      "assignee_key": "code_review"
    }
