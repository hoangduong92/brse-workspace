{
  "name": "brsekit-starter",
  "version": "1.1.0",
  "description": "BrseKit Starter - AI-powered toolkit for Bridge System Engineers",
  "author": "BrseKit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brsekit/brsekit-starter"
  },
  "skills": [
    {
      "name": "bk-track",
      "description": "Project tracking - status analysis and weekly reports with PPTX support",
      "commands": ["status", "report", "summary"]
    },
    {
      "name": "bk-capture",
      "description": "Capture tasks and meeting minutes from text, emails, transcripts",
      "commands": ["task", "meeting", "email"]
    },
    {
      "name": "bk-spec",
      "description": "Requirement analysis and test document generation",
      "commands": ["analyze", "test", "feature"]
    },
    {
      "name": "bk-recall",
      "description": "Memory layer - store, search, and summarize project context",
      "commands": ["sync", "search", "summary", "list"]
    },
    {
      "name": "bk-convert",
      "description": "JA<->VI translation with glossary support",
      "commands": ["text", "excel"]
    },
    {
      "name": "bk-init",
      "description": "BrseKit project setup wizard",
      "commands": ["setup"]
    },
    {
      "name": "bk-morning",
      "description": "Morning brief for BrSE",
      "commands": ["brief"]
    },
    {
      "name": "bk-write",
      "description": "Generate Japanese business documents",
      "commands": ["email", "report", "spec"]
    },
    {
      "name": "brsekit",
      "description": "BrseKit help and documentation",
      "commands": ["help", "install", "skills", "glossary"]
    }
  ],
  "dependencies": {
    "python": ">=3.9",
    "node": ">=18.0.0"
  },
  "installedAt": null,
  "source": "brsekit-cli"
}
